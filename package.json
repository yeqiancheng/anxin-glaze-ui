{
  "name": "anxin-glaze-ui",
  "version": "1.0.2",
  "description": "React components library",
  "keywords": [
    "react",
    "ui",
    "components",
    "typescript"
  ],
  "author": "anxinbuxinan <yqc852594498@qq.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/anxin-personal-project/cream-design.git"
  },
  "homepage": "https://gitee.com/anxin-personal-project/cream-design/blob/master/cream-design/README.md",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "axios": "^1.11.0",
    "classnames": "^2.5.1",
    "react-transition-group": "^4.4.5",
    "web-vitals": "^2.1.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 5",
    "clean": "rimraf ./dist",
    "clean:storybook": "rimraf ./storybook-static",
    "build": "npm run clean && npm run build-ts && npm run build-css",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "sass ./src/styles/index.scss ./dist/index.css",
    "build:storybook": "npm run clean:storybook && npm run build-storybook",
    "test": "react-scripts test",
    "test:nowatch": "cross-env CI=true react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublish": "npm run test:nowatch && npm run lint && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:nowatch && npm run lint"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(axios)/)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/preset-create-react-app": "^9.1.2",
    "@storybook/react-webpack5": "^9.1.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^18.1.9",
    "@types/react-dom": "^18.1.7",
    "@types/react-transition-group": "^4.4.12",
    "cross-env": "^10.0.0",
    "eslint-plugin-storybook": "^9.1.2",
    "husky": "^9.1.7",
    "prop-types": "^15.8.1",
    "react": "^18.1.1",
    "react-dom": "^18.1.1",
    "react-scripts": "5.0.1",
    "rimraf": "^6.0.1",
    "sass": "^1.33.0",
    "storybook": "^9.1.2",
    "typescript": "^4.9.5",
    "webpack": "^5.101.0"
  }
}
